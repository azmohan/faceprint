<resources>

    <dimen name="droi_status_bar_recents_app_icon_max_width">48dp</dimen>
    <!-- Upper height limit for application icon -->
    <dimen name="droi_status_bar_recents_app_icon_max_height">48dp</dimen>

    <!-- Size of application thumbnail -->
    <dimen name="droi_status_bar_recents_thumbnail_width">180dp</dimen>
    <dimen name="droi_status_bar_recents_thumbnail_height">320dp</dimen>
    <dimen name="droi_status_bar_recents_thumbnail_bg_padding">4dp</dimen>

    <!-- Size of application label text -->
    <dimen name="droi_status_bar_recents_app_label_text_size">14dip</dimen>
    <!-- Size of application description text -->
    <dimen name="droi_status_bar_recents_app_description_text_size">14dip</dimen>
    <!-- Size of fading edge for text -->
    <dimen name="droi_status_bar_recents_text_fading_edge_length">20dip</dimen>
    <!-- Size of fading edge for scrolling -->
    <dimen name="droi_status_bar_recents_scroll_fading_edge_length">10dip</dimen>
    <!-- Margin between recents container and glow on the right -->
    <dimen name="droi_status_bar_recents_right_glow_margin">100dip</dimen>
    <!-- How far the thumbnail for a recent app appears from left edge -->
    <dimen name="droi_status_bar_recents_thumbnail_left_margin">20dp</dimen>
    <!-- Padding for text descriptions -->
    <dimen name="droi_status_bar_recents_text_description_padding">8dp</dimen>
    <!-- Width of application label text -->
    <dimen name="droi_status_bar_recents_app_label_width">88dip</dimen>
    <!-- Left margin of application label text -->
    <dimen name="droi_status_bar_recents_app_label_left_margin">0dip</dimen>
    <!-- Padding between recents items -->
    <dimen name="droi_status_bar_recents_item_padding">0dip</dimen>
    <!-- When recents first appears, how far the icon and label of the primary activity
         travel -->
    <dimen name="droi_status_bar_recents_app_icon_translate_distance">35dip</dimen>

    <!-- Where to place the app icon over the thumbnail -->
    <dimen name="droi_status_bar_recents_app_icon_left_margin">0dp</dimen>
    <dimen name="droi_status_bar_recents_app_icon_top_margin">8dp</dimen>

    <!-- The size of the application icon in the recents task view. -->
    <dimen name="droi_recents_task_view_application_icon_size">48dp</dimen>

    <!-- The radius of the rounded corners on a task view. -->
    <dimen name="droi_recents_task_view_rounded_corners_radius">2dp</dimen>

    <!-- The min translation in the Z index for the last task. -->
    <dimen name="droi_recents_task_view_z_min">20dp</dimen>

    <!-- The max translation in the Z index for the last task. -->
    <dimen name="droi_recents_task_view_z_max">80dp</dimen>

    <!-- The amount to translate when animating the removal of a task. -->
    <dimen name="droi_recents_task_view_remove_anim_translation_x">100dp</dimen>

    <!-- The amount of highlight to make on each task view. -->
    <dimen name="droi_recents_task_view_highlight">1.5dp</dimen>

    <!-- The amount to offset when animating into an affiliate group. -->
    <dimen name="droi_recents_task_view_affiliate_group_enter_offset">64dp</dimen>

    <!-- The alpha to apply to a task thumbnail. -->
    <item name="droi_recents_task_view_thumbnail_alpha" format="float" type="dimen">0.9</item>

    <!-- The height of a task view bar. -->
    <dimen name="droi_recents_task_bar_height">56dp</dimen>

    <!-- The height of the search bar space. -->
    <dimen name="droi_recents_search_bar_space_height">64dp</dimen>

    <!-- The side padding for the task stack as a percentage of the width. -->
    <item name="droi_recents_stack_width_padding_percentage" format="float" type="dimen">0.03333</item>

    <!-- The overscroll percentage allowed on the stack. -->
    <item name="droi_recents_stack_overscroll_percentage" format="float" type="dimen">0.0875</item>

    <!-- The top offset for the task stack. -->
    <dimen name="droi_recents_stack_top_padding">16dp</dimen>

    <!-- The dimesnsions of the dismiss all recents button. -->
    <dimen name="droi_recents_dismiss_all_button_size">48dp</dimen>

    <!-- The min alpha to apply to a task affiliation group color. -->
    <item name="droi_recents_task_affiliation_color_min_alpha_percentage" format="float" type="dimen">0.6</item>

    <!-- The size of the lock-to-app button. -->
    <dimen name="droi_recents_lock_to_app_size">56dp</dimen>

    <!-- The size of the lock-to-app button icon. -->
    <dimen name="droi_recents_lock_to_app_icon_size">28dp</dimen>

    <!-- Screen pinning request width -->
    <dimen name="droi_screen_pinning_request_width">-1px</dimen>
    <!-- Screen pinning request nav button circle heights -->
    <dimen name="droi_screen_pinning_request_button_height">66dp</dimen>
    <!-- Screen pinning request nav button circle widths -->
    <dimen name="droi_screen_pinning_request_button_width">84dp</dimen>
    <!-- Screen pinning request padding on top of inner circle -->
    <dimen name="droi_screen_pinning_request_inner_padding">14dp</dimen>
    <!-- Screen pinning request padding on top of icons -->
    <dimen name="droi_screen_pinning_request_nav_icon_padding">18dp</dimen>
    <!-- Screen pinning request padding on side of icons
         (makes the width match the nav bar)-->
    <dimen name="droi_screen_pinning_request_nav_side_padding">7dp</dimen>
    <!-- Screen pinning request side views to match nav bar
         navigation_side_padding - 3 / 2 * (screen_pinning_request_button_width
                                                - navigation_key_width) -->
    <dimen name="droi_screen_pinning_request_side_width">34dp</dimen>
    <!-- Screen pinning request controls padding on bigger screens -->
    <dimen name="droi_screen_pinning_request_frame_padding">0dp</dimen>
    <!-- Screen pinning inner nav bar circle size -->
    <dimen name="droi_screen_pinning_nav_highlight_size">56dp</dimen>
    <!-- Screen pinning inner nav bar outer circle size -->
    <dimen name="droi_screen_pinning_nav_highlight_outer_size">84dp</dimen>

    <dimen name="droi_fake_shadow_size">8dp</dimen>

    <dimen name="droi_status_bar_height">@*android:dimen/status_bar_height</dimen>
    <dimen name="droi_navigation_bar_height">@*android:dimen/navigation_bar_height</dimen>

    <dimen name="droi_app_thumbnail_title_height">20dp</dimen>

    <dimen name="droi_recent_task_mem_circular_diameter">48dp</dimen>

    <dimen name="droi_status_bar_recents_item_width">400px</dimen>
    <dimen name="droi_status_bar_recents_height">640px</dimen>

    <dimen name="droi_qs_time_size">30sp</dimen>

    <dimen name="droi_multi_user_avatar_size">24dp</dimen>

    <!-- freeme,gouzhouping, 20160621. Battery location, Default 1dp --> 
    <dimen name="droi_battery_level_padding">0dp</dimen>

    <!-- Droi Override --><dimen name="qs_panel_padding_bottom">2dp</dimen>

    <!-- radius of the corners of the material rounded rect background -->
    <!-- Droi Override --><dimen name="notification_material_rounded_rect_radius">0dp</dimen>

    <!-- The width of user avatar when on Keyguard -->
    <!-- Droi Override --><dimen name="multi_user_avatar_keyguard_size">24dp</dimen>

    <!-- The width of user avatar when expanded -->
    <!-- Droi Override --><dimen name="multi_user_avatar_expanded_size">48dp</dimen>

    <!-- Margin on the left side of the carrier text on Keyguard -->
    <!-- Droi Override --><dimen name="keyguard_carrier_text_margin">8dp</dimen>

    <!-- Height of the status bar header bar -->
    <!-- freeme.gouzhouping, 20160613. Header height, Default 40dp -->
    <!-- Droi Override --><dimen name="status_bar_header_height">60dp</dimen>

    <!-- Modified begin by droi xupeng 20160421,fix bug tyd00598659, no space between clock and data speed -->
    <dimen name="droi_padding_clock_speed">5dp</dimen>
    <!-- Modified end -->

    <dimen name="droi_network_speed_padding_bottom">1dp</dimen>
    <dimen name="freeme_float_window_width">47.5dip</dimen>
    <dimen name="freeme_float_window_height">@dimen/freeme_float_window_width</dimen>
</resources>
