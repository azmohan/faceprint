    <fraction name="battery_subpixel_smoothing_right">12.5%</fraction>

    <dimen name="battery_margin_bottom">0dp</dimen>
    
    <!-- freeme, gouzhouping, 20160923. between app thumbnail to clear button and indicator height-->
