<dashboard-categories
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:settings="http://schemas.android.com/apk/res/com.android.settings">

    <dashboard-category
            android:id="@+id/other_dashboard_tile"
            android:title="@string/other_dashboard_tile" >

        <!-- Wifi -->
        <dashboard-tile
                android:id="@+id/wifi_settings"
                android:title="@string/wifi_settings_title"
                android:fragment="com.android.settings.wifi.WifiSettings"
                android:icon="@drawable/ic_settings_wireless_2"
                />

        <!-- Bluetooth -->
        <dashboard-tile
                android:id="@+id/bluetooth_settings"
                android:title="@string/bluetooth_settings_title"
                android:fragment="com.android.settings.bluetooth.BluetoothSettings"
                android:icon="@drawable/ic_settings_bluetooth_2"
                />

        <!-- Other wireless and network controls -->
        <dashboard-tile
                android:id="@+id/wireless_settings"
                android:title="@string/radio_control_sim_title"
                android:fragment="com.android.settings.WirelessSettings"
                android:icon="@drawable/ic_sim_sd_2"
                />

        <!-- zhuoyi Account -->
        <dashboard-tile
                android:id="@+id/zhuoyi_account_settings"
                android:title="@string/login_account_droi"
                android:icon="@drawable/ic_settings_accounts_2"
                />
        
        <!-- Location -->
        <dashboard-tile
                android:id="@+id/location_settings"
                android:title="@string/location_settings_title"
                android:fragment="com.android.settings.location.LocationSettings"
                android:icon="@drawable/ic_settings_location_2"
                />

        <!-- Security -->
        <dashboard-tile
                android:id="@+id/security_settings"
                android:title="@string/security_settings_title"
                android:fragment="com.android.settings.SecuritySettings"
                android:icon="@drawable/ic_settings_security_2"
                />

        <dashboard-tile
                android:id="@+id/accessibility_settings_ext"
                android:fragment="com.android.settings.accessibility.AccessibilitySettingsExt"
                android:title="@string/accessibility_settings_ext_title"
                android:icon="@drawable/ic_settings_accessibility_ext_2" />

        <!-- Display -->
        <dashboard-tile
                android:id="@+id/display_settings"
                android:title="@string/display_settings"
                android:fragment="com.android.settings.DisplaySettings"
                android:icon="@drawable/ic_settings_display_2"
                />

        <!-- Sound 
        <dashboard-tile
                android:id="@+id/notification_settings"
                android:title="@string/notification_settings"
                android:fragment="com.mediatek.audioprofile.AudioProfileSettings"
                android:icon="@drawable/ic_settings_notifications_2"
                />
        -->

        <!-- Sound -->
        <!--Modified by tyd xiaocui 2015-09-15 for 6.0 style audioprofile-->
        <dashboard-tile
                android:id="@+id/notification_settings"
                android:title="@string/notification_settings"
                android:fragment="com.mediatek.audioprofile.Editprofile"
                android:icon="@drawable/ic_settings_notifications_2"
                />
       <!--Modified by tyd xiaocui 2015-09-15 for 6.0 style audioprofile end--> 
        <!-- Lock Screen & Password -->
        <dashboard-tile
                android:id="@+id/lock_pwd_settings"
                android:title="@string/lock_pwd_title"
                android:fragment="com.android.settings.LockScreenAndPassword"
                android:icon="@drawable/ic_settings_lock_2"
                />

        <!-- Application Settings -->
        <dashboard-tile
                android:id="@+id/application_settings"
                android:title="@string/applications_settings"
                android:fragment="com.android.settings.applications.ManageApplications"
                android:icon="@drawable/ic_settings_applications_2"
                />

        <dashboard-tile
            android:id="@+id/statusbar_settings"
            android:title="@string/statusbar_settings"
            android:icon="@drawable/ic_settings_statusbar_2">
            <intent android:action="com.android.systemui.QuickSettingsPreferenceActivity" />
        </dashboard-tile>

        <!-- Hotknot -->
        <dashboard-tile
                android:id="@+id/hotknot_settings"
                android:title="@string/hotknot_settings_title"
                android:fragment="com.mediatek.settings.hotknot.HotKnotSettings"
                android:icon="@drawable/ic_settings_hotknot_2" />

        <!-- Language -->
        <dashboard-tile
                android:id="@+id/language_settings"
                android:title="@string/language_settings"
                android:fragment="com.android.settings.inputmethod.InputMethodAndLanguageSettings"
                android:icon="@drawable/ic_settings_language_2"
                />

        <!-- Date & Time -->
        <dashboard-tile
                android:id="@+id/date_time_settings"
                android:title="@string/date_and_time_settings_title"
                android:fragment="com.android.settings.DateTimeSettings"
                android:icon="@drawable/ic_settings_date_time_2"
                />

        <!-- Account -->
        <dashboard-tile
                android:id="@+id/account_settings"
                android:title="@string/account_settings_title"
                android:fragment="com.android.settings.accounts.AccountSettings"
                android:icon="@drawable/ic_settings_other_account_2"
                />

        <!-- Backup and reset -->
        <dashboard-tile
                android:id="@+id/privacy_settings"
                android:title="@string/privacy_settings"
                android:fragment="com.android.settings.PrivacySettings"
                android:icon="@drawable/ic_settings_backup_2"
                />

        <!-- Storage & Battery-->
        <dashboard-tile
                android:id="@+id/storage_battery_settings"
                android:title="@string/storage_battery_settings"
                android:fragment="com.android.settings.deviceinfo.StorageAndBatterySettings"
                android:icon="@drawable/ic_settings_storage_2"
                />
        <!--freeme.shancong,20170804 add storage_settings and battery_settings-->
        <!-- Storage -->
        <dashboard-tile
            android:id="@+id/storage_settings"
            android:title="@string/storage_settings"
            android:fragment="com.android.settings.deviceinfo.StorageSettings"
            android:icon="@drawable/ic_settings_memory"
            />

        <!-- Battery -->
        <dashboard-tile
            android:id="@+id/battery_settings"
            android:title="@string/power_usage_summary_title"
            android:fragment="com.android.settings.fuelgauge.PowerUsageSummary"
            android:icon="@drawable/ic_settings_storage"
            />
        
        <!-- Added by TYD Sean 2015-08-06 for fp begin -->

        <!-- About Device
        <dashboard-tile
                android:id="@+id/about_settings"
                android:title="@string/about_settings"
                android:fragment="com.android.settings.DeviceInfoSettings"
                android:icon="@drawable/ic_settings_about_2"
                />-->
        
        <!-- Other system settingcontrols
        <dashboard-tile
                android:id="@+id/system_settings"
                android:title="@string/radio_controls_title"
                android:fragment="com.android.settings.MoreSystemSettings"
                android:icon="@drawable/ic_system_settings_more_2"
                /> -->

	<!-- Manage users -->
        <dashboard-tile
                android:id="@+id/user_settings"
                android:title="@string/user_settings_title"
                android:fragment="com.android.settings.users.UserSettings"
                android:icon="@drawable/ic_settings_multiuser"
                />

        <!-- Manage NFC payment apps -->
        <dashboard-tile
                android:id="@+id/nfc_payment_settings"
                android:title="@string/nfc_payment_settings_title"
                android:fragment="com.android.settings.nfc.PaymentSettings"
                android:icon="@drawable/ic_settings_nfc_payment"
                />

        <!-- Manufacturer hook -->
        <dashboard-tile
                android:id="@+id/manufacturer_settings"
                android:fragment="com.android.settings.WirelessSettings">
            <intent android:action="com.android.settings.MANUFACTURER_APPLICATION_SETTING" />
        </dashboard-tile>

	<!--HetComm-->
	    <!-- freeme. xiaocui,20160621 delete hetcomm menu
        <dashboard-tile
                android:id="@+id/hetcomm_settings"
                android:icon="@drawable/ic_settings_hetcomm"
                android:title="@string/hetcom_setting_title">
            <intent android:action="com.android.settings.HETCOMM_SETTINGS" />
        </dashboard-tile>
        -->

	<!-- Operator hook -->
        <dashboard-tile
                android:id="@+id/operator_settings"
                android:fragment="com.android.settings.WirelessSettings" >
            <intent android:action="com.android.settings.OPERATOR_APPLICATION_SETTING" />
        </dashboard-tile>

    </dashboard-category>

</dashboard-categories>
